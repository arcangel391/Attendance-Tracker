<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.attendancetracker">
    
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"

        android:theme="@style/Theme.AttendanceTracker">
        <uses-library android:name="org.apache.http.legacy" android:required="false" />
        <!-- <activity
            android:name=".Navbar"
            android:exported="false" />
        <activity android:name=".LoginPage" />
         -->
        <!--
<activity
            android:name=".SplashScreen"
            android:exported="false" />
        -->
        <activity
            android:name=".Dashboard"
            android:exported="false"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/Theme.AttendanceTracker.NoActionBar" />
        <activity android:name=".Navbar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SubmitProject"
            android:usesCleartextTraffic="true"
            />
        <activity android:name=".ProjectView"
            android:usesCleartextTraffic="true"
            />
        <activity android:name=".EditProject"
            android:usesCleartextTraffic="true"
            />
    </application>

</manifest>